   <media:title>Sci-Fi Fantasy Short Film: &quot;Penumbra&quot; | DUST</media:title>
   <media:content url="https://www.youtube.com/v/EKUqwOMYvgs?version=3" type="application/x-shockwave-flash" width="640" height="390"/>
